{
  "expo": {
    "name": "jobs",
    "description": "Jobs React Native Expo app using WorkBC Jobs API to search on map and show a stack of cards containing details of the job posting to swipe left to ignore and swipe right to like and add to a list of Jobs to review later.",
    "slug": "jobs",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manijafari.jobsexpo"
    },
    "android": {
      "package": "com.manijafari.jobsexpo",
      "useNextNotificationsApi": true
    }
  }
}
